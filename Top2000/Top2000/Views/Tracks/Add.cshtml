@model Top2000.Models.Tracks

@{
    ViewBag.Title = "Add";
}

<h2>Create</h2>


@using (Html.BeginForm()) 
{
    @Html.AntiForgeryToken()
    
<div class="form-horizontal">
    <h4>Tracks</h4>
    <hr />
    @Html.ValidationSummary(true, "", new { @class = "text-danger" })
    <div class="form-group">
        <label class="control-label col-md-2" for="trackId">Id</label>
        <div class="col-md-10">
            @Html.EditorFor(model => model.trackId, new { htmlAttributes = new { @class = "form-control readonlyfield", @Readonly = "readonly" } })
            @Html.ValidationMessageFor(model => model.trackId, "", new { @class = "text-danger" })
        </div>
    </div>
    <div class="form-group">
        <label class="control-label col-md-2" for="Listing">list year</label>
        <div class="col-md-10">
            @Html.DropDownList("Listing", null, htmlAttributes: new { @class = "form-control" })
            @Html.ValidationMessageFor(model => model.Listing, "", new { @class = "text-danger" })
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">
            <input type="submit" value="Add" class="btn btn-default" />
        </div>
    </div>
</div>
}

<div>
    @Html.ActionLink("Back to List", "Index")
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
